@model ContactUsViewModel

@{
    Layout = "_AdminLayout";
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Contact Us</h1>
</div>

<form method="post">
    <div class="form-group">
        <label asp-for="Address"></label>
        <input asp-for="Address" class="form-control"  aria-describedby="emailHelp" placeholder="Address">
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control"  aria-describedby="emailHelp" placeholder="Email">
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control"  aria-describedby="emailHelp" placeholder="PhoneNumber">
    </div>
    <div class="form-group">
        <label asp-for="Latitude"></label>
        <input asp-for="Latitude" class="form-control"  aria-describedby="emailHelp" placeholder="Latitude">
    </div>
    <div class="form-group">
        <label asp-for="Longitude"></label>
        <input asp-for="Longitude" class="form-control"  aria-describedby="emailHelp" placeholder="Longitude">
    </div>
   <button class="btn btn-success mb-5" type="submit">Save</button>
</form>